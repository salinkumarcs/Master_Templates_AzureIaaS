{
    "$schema": "http://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "subscriptionID": {
            "value": "<<ProdSubscriptionID>>"
        },
        "rgName": {
            "value": "<<customerCode>>-UAE-PRD-ARG-Security"
        },
        "rgLocation": {
            "value": "UAE North"
        },
        "rgTags": {
            "value": {
                "Environment": "Production",
                "Owner": "<<customerCode>> IT"
            }
        },
        "rgPermissions": {
            "value": [{
                    "builtInRoleType": "Contributor",
                    "roleid": "<<azureADGroupID>>"
                }]
        },
        "boolAzureADRBAC": {
            "value": false
        },
        "boolResourceLock": {
            "value": false
        },
        "nestedTemplates": {
            "value": {
                "deploymentsLoop1": [
                    {
                        "name": "keyVault",
                        "template_uri": "https://<<preCreatedStorageAccount>>.blob.core.windows.net/<<preCreatedStorageContainer>>/keyVault.json",
                        "template_parameters_uri": "https://<<preCreatedStorageAccount>>.blob.core.windows.net/<<preCreatedStorageContainer>>/parameters/keyVault.Prod.parameters.json"
                    }
                ],
                "deploymentsLoop2": [
                    {
                        "name": "azurePolicy",
                        "template_uri": "https://<<preCreatedStorageAccount>>.blob.core.windows.net/<<preCreatedStorageContainer>>/azurePolicy.json",
                        "template_parameters_uri": "https://<<preCreatedStorageAccount>>.blob.core.windows.net/<<preCreatedStorageContainer>>/parameters/azurePolicy.Security.Prod.parameters.json"
                    }
                ],
                "deploymentsLoop3": []
            }
        },
        "_sasToken": {
            "value": "<<sasTokenfromPreCreatedStorageAccount>>"
        }
    }
}
